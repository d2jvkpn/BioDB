<!doctype html>
<html>

<head>
  <meta charset = "utf-8">
  <meta http-equiv = "Content-Type"  content="text/html; charset=utf-8">

  <title>BioDB Genome Results</title>
</head>

<body>
  <br></br>
  
  <div align="center">
    <p>
      <i> <b>Genome</b></i> results of <i><b>{{.Taxon}}</b> </i>
    </p>
  </div>

  <br></br>
  <div style="left: 10%; width: 80%; position: absolute">
    <hr style="border:1 dashed #987cb9" color=#987cb9 size=1>

    {{range .Genomelist}}
      <!--p><b>Taxon id</b>: {{.Taxon_id}}</p-->

      <p><b>Organism name</b>: {{.Organism_name}}</p>
      <p><b>URL</b>: <a href="{{.URL}}">{{.URL}}<a></p>
      <p><b>Information</b>: {{.Information}}</p>
 
      <hr style="border:1 dashed #987cb9" color=#987cb9 size=1>
    {{end}}

</body>
</html>
